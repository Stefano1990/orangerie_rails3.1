<% @category.pages.each do |page| %>
	<tr>
			<td><%= page.position %></td>
			<td><%= link_to page.title, edit_admin_page_path(page) %></td>
			<td><%= button_to "Move up", move_up_admin_page_path(page), 
					:method => :put,
					:id => "move_up" %></td>
			<td><%= button_to "Move down", move_down_admin_page_path(page), 
					:method => :put, 
					:id => "move_down" %></td>
			<td><%= button_to "DELETE", admin_page_path(page), :method => :delete, 
					:class => "destroy",
					:id => "destroy_link" %></td>
	</tr>
<% end %>